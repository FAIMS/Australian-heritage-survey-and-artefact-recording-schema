/******* new record button in boundary tab ****/

addOnEvent("Surface_Exposure/General", "leave", "validateTabGps()");
addOnEvent("Surface_Exposure_Boundary_point/Surface_Exposure_Boundary_point", "leave", "validateTabGps()");

void validateTabGps(){
	current_tab = getDisplayedTab();
	if (isNull(current_tab+"/Latitude") || isNull(current_tab+"/Longitude")) {
		showWarning("Missing Location!", "Make sure you add location data to this entity!");
	}

} 


